<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<LangVersion>preview</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>Sero.Loxy</Title>
		<PackageProjectUrl>https://github.com/Agnael/Sero.Loxy.EfCore</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Agnael/Sero.Loxy.EfCore</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="7.0.4" />
	  <PackageReference Include="Sero.Loxy" Version="2.0.24" />
	</ItemGroup>
</Project>
